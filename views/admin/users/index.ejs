<%- include ('../../partials/header.ejs') %>
<%- include ('../../partials/navbar.ejs') %>
<div class="container">
    
    <h2 class="mt-4">Usuários</h2>
    <a href="/admin/users/create"><button class="btn btn-success">Criar novo Usuário</button></a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(users =>{ %>
                <tr>
                    <th><%= users.id %></th>
                    <th><%= users.email %></th>
                    </th>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>
<%- include ('../../partials/footer.ejs') %>

